<odoo>
    <data>

        <record id="car_report_form_view_wiz" model="ir.ui.view">
            <field name="name">car.report.form.view.wiz</field>
            <field name="model">car.report.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="horse_power_update_test"/>
                    </group>
                    <footer>
                        <button string="Report Test" type="object" name="export_report_test"/>
                        <button string="Cancel" special="cancel" class="btn btn-secondary"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="car_report_action" model="ir.actions.act_window">
            <field name="name">Car Report</field>
            <field name="res_model">car.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>



    </data>
</odoo>